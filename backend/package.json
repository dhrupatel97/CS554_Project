{
  "name": "backend",
  "version": "1.0.0",
  "description": "Backend of Artsy",
  "main": "app.js",
  "scripts": {
    "seed": "mongorestore -d Artsy db/Artsy",
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "tsc",
    "client-install": "npm install --prefix ../client",
    "client": "npm start --prefix ../client",
    "server": "ts-node ./src/server.ts",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "prod": "concurrently \"npm run build && npm start\" \"npm run client\"",
    "start": "node ./dist/src/server.js",
    "watch": "nodemon --exec ts-node -- ./src/server.ts"
  },
  "author": "Dhruv, Akhil, Meghana, Tejashree",
  "license": "ISC",
  "dependencies": {
    "@tensorflow/tfjs": "^2.7.0",
    "@types/cors": "^2.8.8",
    "@types/express": "^4.17.9",
    "@types/mongodb": "^3.5.34",
    "@types/mongoose": "^5.10.1",
    "@types/node": "^14.14.10",
    "@types/request": "^2.48.5",
    "aws-sdk": "^2.799.0",
    "body-parser": "^1.19.0",
    "concurrently": "^5.3.0",
    "config": "^3.3.2",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "firebase-admin": "^9.4.2",
    "fs": "0.0.1-security",
    "helmet": "^4.2.0",
    "image-size": "^0.9.3",
    "imagemagick": "^0.1.3",
    "jsonwebtoken": "^8.5.1",
    "mongodb": "^3.6.3",
    "mongoose": "^5.10.15",
    "multer": "^1.4.2",
    "multer-s3": "^2.9.0",
    "nodemon": "^2.0.6",
    "path": "^0.12.7",
    "probe-image-size": "^6.0.0",
    "request": "^2.88.2",
    "ts-node": "^9.1.1",
    "typescript": "^4.1.2",
    "upscaler": "^0.7.4",
    "xss-clean": "^0.1.1"
  }
}
